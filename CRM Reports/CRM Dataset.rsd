<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>CRM Data Source</DataSourceReference>
      <CommandText>SELECT        tbl_assigntask.*, tbl_lead.*
FROM            tbl_lead INNER JOIN
                         tbl_assigntask ON tbl_lead.leadno = tbl_assigntask.leadno</CommandText>
    </Query>
    <Fields>
      <Field Name="leadno">
        <DataField>leadno</DataField>
        <rd:TypeName>System.Int64</rd:TypeName>
      </Field>
      <Field Name="leadname">
        <DataField>leadname</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="assigntask">
        <DataField>assigntask</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="deadline">
        <DataField>deadline</DataField>
        <rd:TypeName>System.DateTime</rd:TypeName>
      </Field>
      <Field Name="Expr1">
        <DataField>Expr1</DataField>
        <rd:TypeName>System.Int64</rd:TypeName>
      </Field>
      <Field Name="Expr2">
        <DataField>Expr2</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="infofrom">
        <DataField>infofrom</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="address">
        <DataField>address</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="landmark">
        <DataField>landmark</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="city">
        <DataField>city</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="contact">
        <DataField>contact</DataField>
        <rd:TypeName>System.Decimal</rd:TypeName>
      </Field>
      <Field Name="business">
        <DataField>business</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="opportunity">
        <DataField>opportunity</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="remarks">
        <DataField>remarks</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>